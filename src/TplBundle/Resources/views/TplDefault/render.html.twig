{% extends 'tpl.html.twig' %}
{% block body %}
	{{parent()}}
	<div class="container-fluid tpl">
		<div class="row">
			<div class="tpl_default">
				{{render(controller('AppBundle:Mod:render',{mod: params.mods.p1}))}}
			</div>
		</div>
	</div>
{% endblock %}

{% block stylesheets %}
	{{parent()}}
	<style type="text/css">
		.tpl { max-width: 700px; margin: 20px auto;}
		.mod-body { margin-bottom: 15px; }
		.mod-form button.close{ 
		  padding:0; 
		  margin: 0px 13px 0 0;
		}
	</style>
{% endblock %}

{% block javascripts %}
	{{parent()}}
	{% javascripts '@TplBundle/Resources/public/js/*' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
